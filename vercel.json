{
    "version": 2,
    "builds": [
      {
        "src": "./lmsclient/package.json",
        "use": "@vercel/static-build" 
      },
      
      {
       "src": "./server/server.ts",
       "use": "@vercel/node"
     }
    
    ],
     "rewrites": [
     {
      "source": "/(.*)",
      "destination": "/server/server.ts"
     },
     {
      "source": "/(.*)",
      "destination": "/lmsclient/dist/$1"
      }
    ]
   }