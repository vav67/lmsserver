{
    "version": 2,
    "builds": [
     {
       "src": "./server.ts",
       "use": "@vercel/node"
     },
     {
        "src": "lmsclient/package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "build"
          }
      }
    ],
     "routes": [
     {
       "src": "/(.*)",
       "dest": "/server.ts"
     },
     {
        "src": "/(.*)",
        "dest": "/lmsclient/dist/$1"
      }
    ]
   }